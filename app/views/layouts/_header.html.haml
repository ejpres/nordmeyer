%header
  = link_to logo, root_path
  = title
  %nav
    %ul
      %li= link_to "Home", root_path
      %li= link_to "Help", help_path
      - if signed_in?
        %li= link_to "Profile", current_user
        %li= link_to "Sign out", signout_path
      - else
        %li= link_to "Register", register_path
        %li= link_to "Sign in", signin_path